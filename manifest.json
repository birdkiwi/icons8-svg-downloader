{
  "manifest_version": 2,
  "name": "icons8 svg downloader",
  "version": "1.0",

  "description": "Downloads svg icons from icons8.com website",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.icons8.com/*"],
      "js": ["index.js"]
    }
  ]
}